prefix=@NII_PREFIX_PATH@
exec_prefix=${prefix}
libdir=${prefix}/@NII_LIB_DIRECTORY@
includedir=${prefix}/include

Name: NII-PCZ
Description: Portal-Connected Zone Scene Manager for NII
Version: @NII_VERSION@
Requires: NII = @NII_VERSION@
Libs: ${libdir}/NII/@NII_PLUGIN_PREFIX@Plugin_PCZSceneManager@NII_LIB_SUFFIX@@NII_PLUGIN_EXT@ ${libdir}/NII/@NII_PLUGIN_PREFIX@Plugin_OctreeZone@NII_LIB_SUFFIX@@NII_PLUGIN_EXT@ -Wl,-rpath,${libdir}/NII
Cflags: -I${includedir}/NII/Plugins/PCZSceneManager -I${includedir}/NII/Plugins/OctreeZone @NII_CFLAGS@
