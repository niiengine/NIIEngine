            <ImageryCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Operator type="Subtract">
                            <Rel scale="0.5" value="Height"/>
                            <Operator type="Multiply">
                                <PixelSize pixel_id="Vanilla-Images/RadioButtonNormal" value="Height"/>
                                <Operator type="Multiply">
                                    <PropertyDim name="RadioButtonRelativeHeight"/>
                                    <Abs value="0.5"/>
                                </Operator>
                            </Operator>
                        </Operator>
                    </Border>
                    <Border type="Width">
                        <Operator type="Multiply">
                            <PixelSize pixel_id="Vanilla-Images/RadioButtonMark" value="Width"/>
                            <PropertyDim name="RadioButtonRelativeWidth"/>
                        </Operator>
                    </Border>
                    <Border type="Height">
                        <Operator type="Multiply">
                            <PixelSize pixel_id="Vanilla-Images/RadioButtonMark" value="Height"/>
                            <PropertyDim name="RadioButtonRelativeHeight"/>
                        </Operator>
                    </Border>
                </Area>
                <Com pixel_id="Vanilla-Images/RadioButtonMark"/>
                <VLayout type="stretch"/>
                <HLayout type="stretch"/>
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="normal"/>
                <Section style_section="label">
                    <ColourAttribute name="NormalTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Hover">
            <Layer>
                <Section style_section="hover"/>
                <Section style_section="label">
                    <ColourAttribute name="HoverTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="normal">
                    <Colours top_left="FF7F7F7F" bottom_left="FF7F7F7F" top_right="FF7F7F7F" bottom_right="FF7F7F7F"/>
                </Section>
                <Section style_section="label">
                    <ColourAttribute name="DisabledTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="SelectedNormal">
            <Layer>
                <Section style_section="normal"/>
                <Section style_section="select_mark"/>
                <Section style_section="label">
                    <ColourAttribute name="NormalTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="SelectedHover">
            <Layer>
                <Section style_section="hover"/>
                <Section style_section="select_mark"/>
                <Section style_section="label">
                    <ColourAttribute name="HoverTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="SelectDisable">
            <Layer>
                <Section style_section="normal">
                    <Colours top_left="FF7F7F7F" bottom_left="FF7F7F7F" top_right="FF7F7F7F" bottom_right="FF7F7F7F"/>
                </Section>
                <Section style_section="select_mark">
                    <Colours top_left="FF7F7F7F" bottom_left="FF7F7F7F" top_right="FF7F7F7F" bottom_right="FF7F7F7F"/>
                </Section>
                <Section style_section="label">
                    <ColourAttribute name="DisabledTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>

    <WidgetStyle id="Vanilla/Tooltip">
    <Property name="Size" value="{{0, 128}, {0, 32}}" />
        <Special name="TextArea">
            <Area>
                <Border type="LeftEdge" ><PixelSize pixel_id="Vanilla-Images/TooltipLeftEdge" value="Width" /></Border>
                <Border type="TopEdge" ><PixelSize pixel_id="Vanilla-Images/TooltipTopEdge" value="Height" /></Border>
                <Border type="RightEdge" >
                    <Operator type="Subtract">
                        <Rel scale="1" value="RightEdge" />
                        <PixelSize pixel_id="Vanilla-Images/TooltipRightEdge" value="Width" />
                    </Operator>
                </Border>
                <Border type="BottomEdge" >
                    <Operator type="Subtract">
                        <Rel scale="1" value="BottomEdge" />
                        <PixelSize pixel_id="Vanilla-Images/TooltipBottomEdge" value="Height" />
                    </Operator>
                </Border>
            </Area>
        </Special>
        <StyleSection id="main">
            <FrameCom>
                <Com type="TopLeftCorner" pixel_id="Vanilla-Images/TooltipTopLeft" />
                <Com type="TopRightCorner" pixel_id="Vanilla-Images/TooltipTopRight" />
                <Com type="BottomLeftCorner" pixel_id="Vanilla-Images/TooltipBottomLeft" />
                <Com type="BottomRightCorner" pixel_id="Vanilla-Images/TooltipBottomRight" />
                <Com type="LeftEdge" pixel_id="Vanilla-Images/TooltipLeftEdge" />
                <Com type="RightEdge" pixel_id="Vanilla-Images/TooltipRightEdge" />
                <Com type="TopEdge" pixel_id="Vanilla-Images/TooltipTopEdge" />
                <Com type="BottomEdge" pixel_id="Vanilla-Images/TooltipBottomEdge" />
                <Com type="Background" pixel_id="Vanilla-Images/TooltipMiddle" />
            </FrameCom>
        </StyleSection>
        <StyleSection id="label">
            <TextCom>
                <Area>
                    <Border type="LeftEdge" ><PixelSize pixel_id="Vanilla-Images/TooltipLeftEdge" value="Width" /></Border>
                    <Border type="TopEdge" ><PixelSize pixel_id="Vanilla-Images/TooltipTopEdge" value="Height" /></Border>
                    <Border type="RightEdge" >
                        <Operator type="Subtract">
                            <Rel scale="1" value="RightEdge" />
                            <PixelSize pixel_id="Vanilla-Images/TooltipRightEdge" value="Width" />
                        </Operator>
                    </Border>
                    <Border type="BottomEdge" >
                        <Operator type="Subtract">
                            <Rel scale="1" value="BottomEdge" />
                            <PixelSize pixel_id="Vanilla-Images/TooltipBottomEdge" value="Height" />
                        </Operator>
                    </Border>
                </Area>
                <Colours top_left="FF000000" top_right="FF000000" bottom_left="FF000000" bottom_right="FF000000" />
                <VLayout type="centre" />
                <HLayout type="centre" />
            </TextCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="main" />
                <Section style_section="label" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="main" />
                <Section style_section="label" />
            </Layer>
        </StyleLayout>

        <AnimationDefinition name="FadeIn" duration="0.33" replayMode="once">
            <PropertyIDKeyFrameTrack property="Alpha" interpolator="float">
                <KeyFrame position="0" sourceProperty="Alpha" />
                <KeyFrame position="0.33" value="1" progression="linear" />
            </PropertyIDKeyFrameTrack>

            <Subscription event="TooltipActive" action="Start" />
        </AnimationDefinition>

        <AnimationDefinition name="FadeOut" duration="0.33" replayMode="once">
            <PropertyIDKeyFrameTrack property="Alpha" interpolator="float">
                <KeyFrame position="0" sourceProperty="Alpha" />
                <KeyFrame position="0.33" value="0" progression="linear" />
            </PropertyIDKeyFrameTrack>
            <PropertyIDKeyFrameTrack property="Visible" interpolator="bool">
                <KeyFrame position="0" sourceProperty="Visible" />
                <KeyFrame position="0.33" value="false" progression="discrete" />
            </PropertyIDKeyFrameTrack>

            <Subscription event="TooltipInactive" action="Start" />
        </AnimationDefinition>
    </WidgetStyle>
</Style>
