        <Property name="WantsMultiClickEvents" value="False"/>
        <Property name="Size" value="{{1, 0}, {0, 32}}" />
        <Special name="ThumbTrackArea">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="1" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="1" value="Height"/>
                </Border>
            </Area>
        </Special>
        <Widget id="__auto_incbtn__" style_id="DefaultWindow">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="0" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="0" value="Width"/>
                </Border>
            </Area>
            <Property name="Visible" value="False"/>
        </Widget>
        <Widget id="__auto_decbtn__" style_id="DefaultWindow">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="0" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="0" value="Width"/>
                </Border>
            </Area>
            <Property name="Visible" value="False"/>
        </Widget>
        <Widget id="__auto_thumb__" style_id="OgreTray/Thumb">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="1" value="Height"/>
