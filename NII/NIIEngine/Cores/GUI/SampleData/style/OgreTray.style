            <Layer>
                <Section style_section="normal"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Tab control
-->
    <WidgetStyle id="OgreTray/TabControl">
        <Property name="TabButtonType" value="OgreTray/TabButton"/>
        <Property name="TabTextPadding" value="{0,6}"/>
        <Widget id="__auto_TabPane__" style_id="OgreTray/TabContentPane">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="RightEdge">
                    <Rel scale="1" value="Width"/>
                </Border>
                <Border type="TopEdge">
                    <Operator type="Multiply">
                        <PropertyDim widget="__auto_TabPane__" name="EnableBottom"/>
                        <PropertyDim type="Height" name="TabHeight"/>
                    </Operator>
                </Border>
                <Border type="Height">
                    <Operator type="Subtract">
                        <Rel scale="1" value="Height"/>
                        <PropertyDim type="Height" name="TabHeight"/>
                    </Operator>
                </Border>
            </Area>
        </Widget>
        <Widget id="__auto_TabPane__Buttons" style_id="OgreTray/TabButtonPane">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="RightEdge">
                    <Rel scale="1" value="Width"/>
                </Border>
                <Border type="TopEdge">
                    <Operator type="Multiply">
                        <PropertyDim widget="__auto_TabPane__Buttons" name="EnableTop"/>
                        <Operator type="Subtract">
                            <Rel scale="1" value="Height"/>
                            <PropertyDim type="Height" name="TabHeight"/>
                        </Operator>
                    </Operator>
                </Border>
                <Border type="Height">
                    <PropertyDim type="Height" name="TabHeight"/>
                </Border>
            </Area>
        </Widget>
        <Widget id="__auto_TabPane__ScrollLeft" style_id="OgreTray/Button">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <WidgetSize widget="__auto_TabPane__Buttons" value="TopEdge"/>
                </Border>
                <Border type="Width">
                    <Operator type="Multiply">
                        <WidgetSize widget="__auto_TabPane__Buttons" value="Height"/>
                        <Operator type="Divide">
                            <FontDim type="HorzExtent" string="&lt;&lt;"/>
                            <FontDim type="LineSpacing"/>
                        </Operator>
                    </Operator>
                </Border>
                <Border type="BottomEdge">
                    <WidgetSize widget="__auto_TabPane__Buttons" value="BottomEdge"/>
                </Border>
            </Area>
            <Property name="AlwaysOnTop" value="True"/>
            <Property name="Text" value="&lt;&lt;"/>
        </Widget>
        <Widget id="__auto_TabPane__ScrollRight" style_id="OgreTray/Button">
            <Area>
                <Border type="Width">
                    <Operator type="Multiply">
                        <WidgetSize widget="__auto_TabPane__Buttons" value="Height"/>
                        <Operator type="Divide">
                            <FontDim type="HorzExtent" string="&gt;&gt;"/>
                            <FontDim type="LineSpacing"/>
                        </Operator>
                    </Operator>
                </Border>
                <Border type="LeftEdge">
                    <Operator type="Subtract">
                        <Rel scale="1" value="Width"/>
                        <WidgetSize widget="__auto_TabPane__ScrollRight" value="Width"/>
                    </Operator>
                </Border>
                <Border type="TopEdge">
                    <WidgetSize widget="__auto_TabPane__Buttons" value="TopEdge"/>
                </Border>
                <Border type="Height">
                    <WidgetSize widget="__auto_TabPane__Buttons" value="Height"/>
                </Border>
            </Area>
            <Property name="AlwaysOnTop" value="True"/>
            <Property name="Text" value="&gt;&gt;"/>
        </Widget>
        <StyleLayout name="Enable"/>
        <StyleLayout name="Disable"/>
    </WidgetStyle>
</Style>
