                <Com type="Background" pixel_id="OgreTrayImages/HorzTrackM"/>
            </FrameCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="main"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="main"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Shared static widget components.
-->
    <WidgetStyle id="OgreTray/StaticShared">
        <StyleSection id="frame">
            <FrameCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Com type="TopLeftCorner" pixel_id="OgreTrayImages/TextBoxTL"/>
                <Com type="TopRightCorner" pixel_id="OgreTrayImages/TextBoxTR"/>
                <Com type="BottomLeftCorner" pixel_id="OgreTrayImages/TextBoxBL"/>
                <Com type="BottomRightCorner" pixel_id="OgreTrayImages/TextBoxBR"/>
