                    </Border>
                </Area>
                <Com pixel_id="OgreTrayImages/TextBoxM"/>
                <VLayout type="stretch"/>
                <HLayout type="stretch"/>
            </ImageryCom>
        </StyleSection>
        <StyleSection id="no_frame_background">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Com pixel_id="OgreTrayImages/TextBoxM"/>
