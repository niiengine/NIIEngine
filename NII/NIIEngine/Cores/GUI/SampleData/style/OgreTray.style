    <!--
:::: Multi-purpose frame using the MiniTray imagery.
-->
    <WidgetStyle id="OgreTray/MiniTray">
        <PropertyDefine signal_refresh="true" value="FF000000" type="ColourRect" id="TextColour"/>
        <PropertyDefine value="centre" type="VerticalTextFormatting" id="VertFormatting"/>
        <PropertyDefine value="centre" type="HorizontalTextFormatting" id="HorzFormatting"/>
        <StyleSection id="frame">
            <FrameCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Com type="TopLeftCorner" pixel_id="OgreTrayImages/MiniTrayTL"/>
                <Com type="TopRightCorner" pixel_id="OgreTrayImages/MiniTrayTR"/>
                <Com type="BottomLeftCorner" pixel_id="OgreTrayImages/MiniTrayBL"/>
                <Com type="BottomRightCorner" pixel_id="OgreTrayImages/MiniTrayBR"/>
                <Com type="LeftEdge" pixel_id="OgreTrayImages/MiniTrayL"/>
                <Com type="TopEdge" pixel_id="OgreTrayImages/MiniTrayT"/>
                <Com type="RightEdge" pixel_id="OgreTrayImages/MiniTrayR"/>
                <Com type="BottomEdge" pixel_id="OgreTrayImages/MiniTrayB"/>
                <Com type="Background" pixel_id="OgreTrayImages/MiniTrayM"/>
            </FrameCom>
        </StyleSection>
        <StyleSection id="label">
            <TextCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="Width">
                        <Rel scale="1" value="Width"/>
                    </Border>
                    <Border type="Height">
                        <Rel scale="1" value="Height"/>
                    </Border>
                </Area>
                <VertFormatProperty name="VertFormatting"/>
                <HorzFormatProperty name="HorzFormatting"/>
            </TextCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="frame"/>
                <Section style_section="label">
                    <ColourAttribute name="TextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="frame"/>
                <Section style_section="label">
                    <ColourAttribute name="TextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
