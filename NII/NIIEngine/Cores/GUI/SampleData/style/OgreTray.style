<?xml version="1.0" encoding="UTF-8"?>
<!--
    OgreTray.looknfeel by Paul D. Turner

    This looknfeel specifies a set of WidgetStyle definitions that are intended
    to recreate the overall widget_style of Ogre's new SDK tray system within CEGUI.

    This looknfeel has been made for use in some upcoming recreations of the
    Ogre samples, that use CEGUI instead of the new Ogre SDK tray system.  This
    is being done mostly for fun, though also to demonstrate the use of CEGUI
    with Ogre.
-->
<Style ver="7">
    <WidgetStyle id="OgreTray/Label">
        <PropertyDefine signal_refresh="true" value="FF000000" type="Colour" id="NormalTextColour"/>
        <PropertyDefine signal_refresh="true" value="FF444444" type="Colour" id="DisabledTextColour"/>
        <PropertyDefine signal_refresh="true" value="centre" type="VerticalTextFormatting" id="VertFormatting"/>
        <PropertyDefine signal_refresh="true" value="centre" type="HorizontalTextFormatting" id="HorzFormatting"/>
        <Property name="Size" value="{{0, 128}, {0, 32}}" />
        <StyleSection id="Label">
            <TextCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="Width">
                        <Rel scale="1" value="Width"/>
                    </Border>
                    <Border type="Height">
                        <Rel scale="1" value="Height"/>
                    </Border>
                </Area>
                <VertFormatProperty name="VertFormatting"/>
                <HorzFormatProperty name="HorzFormatting"/>
            </TextCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="Label">
                    <ColourAttribute name="NormalTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="Label">
                    <ColourAttribute name="DisabledTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <WidgetStyle id="OgreTray/BGTest">
        <Property name="Size" value="{{0, 128}, {0, 128}}" />
        <StyleSection id="main">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Com pixel_id="OgreTrayImages/BandsFull"/>
                <VLayout type="stretch"/>
                <HLayout type="stretch"/>
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="main">
                    <Colours top_left="FF779988" bottom_left="FF779988" top_right="FF779988" bottom_right="FF779988"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="main"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Titlebar
-->
    <WidgetStyle id="OgreTray/Titlebar">
        <PropertyDefine signal_refresh="true" value="FFe5ffb2" type="ColourRect" id="CaptionColour"/>
        <PropertyDefine value="centre" type="VerticalTextFormatting" id="VertFormatting"/>
        <PropertyDefine value="centre" type="HorizontalTextFormatting" id="HorzFormatting"/>
        <Property name="NonClient" value="True"/>
        <Property name="Size" value="{{0, 256}, {0, 32}}" />
        <StyleSection id="normal">
            <FrameCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Com type="LeftEdge" pixel_id="OgreTrayImages/CaptionL"/>
                <Com type="RightEdge" pixel_id="OgreTrayImages/CaptionR"/>
                <Com type="Background" pixel_id="OgreTrayImages/CaptionM"/>
            </FrameCom>
        </StyleSection>
        <StyleSection id="caption">
            <TextCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <ColourAttribute name="CaptionColour"/>
                <VertFormatProperty name="VertFormatting"/>
                <HorzFormatProperty name="HorzFormatting"/>
            </TextCom>
        </StyleSection>
        <StyleLayout name="Active">
            <Layer>
                <Section style_section="normal"/>
                <Section style_section="caption"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Inactive">
            <Layer>
                <Section style_section="normal"/>
                <Section style_section="caption"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="normal"/>
                <Section style_section="caption"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Frame Window
-->
    <WidgetStyle id="OgreTray/FrameWindow">
        <PropertyLinkDefinition targetProperty="CaptionColour" widget="__auto_titlebar__" value="FFe5ffb2" type="ColourRect" name="CaptionColour"/>
        <PropertyLinkDefinition targetProperty="Font" widget="__auto_titlebar__" type="Font" name="CaptionFont"/>
        <Property name="NSSizingCursorImage" value="OgreTrayImages/MouseArrow"/>
        <Property name="EWSizingCursorImage" value="OgreTrayImages/MouseArrow"/>
        <Property name="NWSESizingCursorImage" value="OgreTrayImages/MouseArrow"/>
        <Property name="NESWSizingCursorImage" value="OgreTrayImages/MouseArrow"/>
        <Property name="AutoRenderingSurface" value="True"/>
        <Property name="Size" value="{{0.3, 0}, {0.3, 0}}" />
        <Special name="ClientWithTitleWithFrame">
            <Area>
                <Border type="LeftEdge">
                    <Operator type="Divide">
                        <PixelSize pixel_id="OgreTrayImages/TrayL" value="Width"/>
                        <Abs value="2"/>
                    </Operator>
                </Border>
                <Border type="TopEdge">
                    <Operator type="Add">
                        <WidgetSize widget="__auto_titlebar__" value="BottomEdge"/>
                        <Operator type="Divide">
                            <PixelSize pixel_id="OgreTrayImages/TrayT" value="Height"/>
                            <Abs value="2"/>
                        </Operator>
                    </Operator>
                </Border>
                <Border type="RightEdge">
                    <Operator type="Subtract">
                        <Rel scale="1" value="RightEdge"/>
                        <Operator type="Divide">
                            <PixelSize pixel_id="OgreTrayImages/TrayR" value="Width"/>
                            <Abs value="2"/>
                        </Operator>
                    </Operator>
                </Border>
                <Border type="BottomEdge">
                    <Operator type="Subtract">
                        <Rel scale="1" value="BottomEdge"/>
                        <Operator type="Divide">
                            <PixelSize pixel_id="OgreTrayImages/TrayB" value="Height"/>
                            <Abs value="2"/>
                        </Operator>
                    </Operator>
                </Border>
            </Area>
        </Special>
        <Special name="ClientWithTitleNoFrame">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <WidgetSize widget="__auto_titlebar__" value="BottomEdge"/>
                </Border>
                <Border type="Width">
                    <Rel scale="1" value="Width"/>
                </Border>
                <Border type="BottomEdge">
                    <WidgetSize value="BottomEdge"/>
                </Border>
            </Area>
        </Special>
        <Special name="ClientNoTitleWithFrame">
            <Area>
                <Border type="LeftEdge">
                    <Operator type="Divide">
                        <PixelSize pixel_id="OgreTrayImages/TrayL" value="Width"/>
                        <Abs value="2"/>
                    </Operator>
                </Border>
                <Border type="TopEdge">
                    <Operator type="Divide">
                        <PixelSize pixel_id="OgreTrayImages/TrayT" value="Height"/>
                        <Abs value="2"/>
                    </Operator>
                </Border>
                <Border type="RightEdge">
                    <Operator type="Subtract">
                        <Rel scale="1" value="RightEdge"/>
                        <Operator type="Divide">
                            <PixelSize pixel_id="OgreTrayImages/TrayR" value="Width"/>
                            <Abs value="2"/>
                        </Operator>
                    </Operator>
                </Border>
                <Border type="BottomEdge">
                    <Operator type="Subtract">
                        <Rel scale="1" value="BottomEdge"/>
                        <Operator type="Divide">
                            <PixelSize pixel_id="OgreTrayImages/TrayB" value="Height"/>
                            <Abs value="2"/>
                        </Operator>
                    </Operator>
                </Border>
            </Area>
        </Special>
        <Special name="ClientNoTitleNoFrame">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="1" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="1" value="Height"/>
                </Border>
            </Area>
        </Special>
        <Widget id="__auto_titlebar__" style_id="OgreTray/Titlebar">
            <Area>
                <Border type="LeftEdge">
                    <Operator type="Divide">
                        <PixelSize pixel_id="OgreTrayImages/TrayL" value="Width"/>
                        <Abs value="2"/>
                    </Operator>
                </Border>
                <Border type="TopEdge">
                    <Operator type="Divide">
                        <PixelSize pixel_id="OgreTrayImages/TrayT" value="Height"/>
                        <Abs value="2"/>
                    </Operator>
                </Border>
                <Border type="Width">
                    <Operator type="Subtract">
                        <Rel scale="1" value="Width"/>
                        <PixelSize pixel_id="OgreTrayImages/TrayL" value="Width"/>
                    </Operator>
                </Border>
                <Border type="Height">
                    <FontDim padding="8" type="LineSpacing"/>
                </Border>
            </Area>
            <Property name="AlwaysOnTop" value="False"/>
        </Widget>
        <!-- here we basically nullify the close button, for now at least -->
        <Widget id="__auto_closebutton__" style_id="DefaultWindow">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="0" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="0" value="Height"/>
                </Border>
            </Area>
            <Property name="NonClient" value="True"/>
        </Widget>
        <StyleSection id="frame">
            <FrameCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Com type="TopLeftCorner" pixel_id="OgreTrayImages/TrayTL"/>
                <Com type="TopRightCorner" pixel_id="OgreTrayImages/TrayTR"/>
                <Com type="BottomLeftCorner" pixel_id="OgreTrayImages/TrayBL"/>
                <Com type="BottomRightCorner" pixel_id="OgreTrayImages/TrayBR"/>
                <Com type="LeftEdge" pixel_id="OgreTrayImages/TrayL"/>
                <Com type="TopEdge" pixel_id="OgreTrayImages/TrayT"/>
                <Com type="RightEdge" pixel_id="OgreTrayImages/TrayR"/>
                <Com type="BottomEdge" pixel_id="OgreTrayImages/TrayB"/>
                <Com type="Background" pixel_id="OgreTrayImages/TrayM"/>
            </FrameCom>
        </StyleSection>
        <StyleSection id="noframe">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="Width">
                        <Rel scale="1" value="Width"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Com pixel_id="OgreTrayImages/TrayM"/>
                <VLayout type="stretch"/>
                <HLayout type="stretch"/>
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="ActiveTitleFrame">
            <Layer>
                <Section style_section="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="InactiveTitleFrame">
            <Layer>
                <Section style_section="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DisableTitleFrame">
            <Layer>
                <Section style_section="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="ActiveTitle">
            <Layer>
                <Section style_section="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="InactiveTitle">
            <Layer>
                <Section style_section="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DisableTitle">
            <Layer>
                <Section style_section="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="ActiveFrame">
            <Layer>
                <Section style_section="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="InactiveFrame">
            <Layer>
                <Section style_section="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DisableFrame">
            <Layer>
                <Section style_section="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Active">
            <Layer>
                <Section style_section="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Inactive">
            <Layer>
                <Section style_section="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="noframe"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: A Thumb for all occasions - can be used by all widgets needing a thumb.
:::: (don't forget to specify free axis in <Widget> definition!)
-->
    <WidgetStyle id="OgreTray/Thumb">
        <Property name="Size" value="{{0, 32}, {0, 32}}" />
        <StyleSection id="main">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="Width">
                        <Rel scale="1" value="Width"/>
                    </Border>
                    <Border type="Height">
                        <Rel scale="1" value="Height"/>
                    </Border>
                </Area>
                <Com pixel_id="OgreTrayImages/Handle"/>
                <VLayout type="stretch"/>
                <HLayout type="stretch"/>
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="main"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Hover">
            <Layer>
                <Section style_section="main"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Pushed">
            <Layer>
                <Section style_section="main"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Vertical Scrollbar
-->
    <WidgetStyle id="OgreTray/VerticalScrollbar">
        <Property name="VerticalScrollbar" value="True"/>
        <Property name="WantsMultiClickEvents" value="False"/>
        <Property name="Size" value="{{0, 32}, {1, 0}}" />
        <Special name="ThumbTrackArea">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="1" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="1" value="Height"/>
                </Border>
            </Area>
        </Special>
        <Widget id="__auto_incbtn__" style_id="DefaultWindow">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="0" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="0" value="Width"/>
                </Border>
            </Area>
            <Property name="Visible" value="False"/>
        </Widget>
        <Widget id="__auto_decbtn__" style_id="DefaultWindow">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="0" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="0" value="Width"/>
                </Border>
            </Area>
            <Property name="Visible" value="False"/>
        </Widget>
        <Widget id="__auto_thumb__" style_id="OgreTray/Thumb">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="Width">
                    <Rel scale="1" value="Width"/>
                </Border>
                <Border type="Height">
                    <Rel scale="1" value="Width"/>
                </Border>
            </Area>
            <Property name="VertFree" value="True"/>
        </Widget>
        <StyleSection id="main">
            <FrameCom>
                <Area>
                    <Border type="LeftEdge">
                        <Rel scale="0.11" value="Width"/>
                    </Border>
                    <Border type="TopEdge">
                        <Rel scale="0.1" value="Width"/>
                    </Border>
                    <Border type="Width">
                        <Rel scale="0.78" value="Width"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Operator type="Subtract">
                            <Rel scale="1" value="BottomEdge"/>
                            <Rel scale="0.1" value="Width"/>
                        </Operator>
                    </Border>
                </Area>
                <Com type="TopEdge" pixel_id="OgreTrayImages/VertTrackT"/>
                <Com type="BottomEdge" pixel_id="OgreTrayImages/VertTrackB"/>
                <Com type="Background" pixel_id="OgreTrayImages/VertTrackM"/>
            </FrameCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="main"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="main"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Horizontal Scrollbar
-->
