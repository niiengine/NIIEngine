        <StyleSection id="progress_empty">
            <FrameCom>
                <Com type="TopLeftCorner" pixel_id="AlfiskoSkin/ProgressEmptyTL"/>
                <Com type="TopRightCorner" pixel_id="AlfiskoSkin/ProgressEmptyTR"/>
                <Com type="BottomLeftCorner" pixel_id="AlfiskoSkin/ProgressEmptyBL"/>
                <Com type="BottomRightCorner" pixel_id="AlfiskoSkin/ProgressEmptyBR"/>
                <Com type="LeftEdge" pixel_id="AlfiskoSkin/ProgressEmptyL"/>
                <Com type="RightEdge" pixel_id="AlfiskoSkin/ProgressEmptyR"/>
                <Com type="TopEdge" pixel_id="AlfiskoSkin/ProgressEmptyT"/>
                <Com type="BottomEdge" pixel_id="AlfiskoSkin/ProgressEmptyB"/>
                <Com type="Background" pixel_id="AlfiskoSkin/ProgressEmptyFill"/>
            </FrameCom>
        </StyleSection>
        <StyleSection id="progress_full">
            <FrameCom>
                <Com type="TopLeftCorner" pixel_id="AlfiskoSkin/ProgressFullTL"/>
                <Com type="TopRightCorner" pixel_id="AlfiskoSkin/ProgressFullTR"/>
                <Com type="BottomLeftCorner" pixel_id="AlfiskoSkin/ProgressFullBL"/>
                <Com type="BottomRightCorner" pixel_id="AlfiskoSkin/ProgressFullBR"/>
                <Com type="LeftEdge" pixel_id="AlfiskoSkin/ProgressFullL"/>
                <Com type="RightEdge" pixel_id="AlfiskoSkin/ProgressFullR"/>
                <Com type="TopEdge" pixel_id="AlfiskoSkin/ProgressFullT"/>
                <Com type="BottomEdge" pixel_id="AlfiskoSkin/ProgressFullB"/>
                <Com type="Background" pixel_id="AlfiskoSkin/ProgressFullFill"/>
            </FrameCom>
        </StyleSection>
        <StyleSection id="progress_shadow">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge">
                        <Operator type="Multiply">
                            <Rel scale="1" value="Width"/>
                            <PropertyDim name="CurrentProgress"/>
                        </Operator>
                    </Border>
                    <Border type="RightEdge">
                        <Operator type="Subtract">
                            <Rel scale="1" value="Width"/>
                            <Abs value="5"/>
                        </Operator>
                    </Border>
                </Area>
                <Com pixel_id="AlfiskoSkin/ProgressShadow"/>
                <VLayout type="stretch"/>
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="progress_empty"/>
                <Section style_section="progress_shadow"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="progress_empty">
                    <Colours top_left="FF7F7F7F" bottom_left="FF7F7F7F" top_right="FF7F7F7F" bottom_right="FF7F7F7F"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="EnableProgress">
            <Layer>
                <Section style_section="progress_full"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DisableProgress">
            <Layer>
                <Section style_section="progress_full">
                    <Colours top_left="FF7F7F7F" bottom_left="FF7F7F7F" top_right="FF7F7F7F" bottom_right="FF7F7F7F"/>
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
