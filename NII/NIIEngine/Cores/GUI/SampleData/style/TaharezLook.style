                <Com pixel_id="TaharezLook/HeaderBarSplitterHover" />
                <VLayout type="stretch" />
                <HLayout type="right" />
            </ImageryCom>
        </StyleSection>
        <StyleSection id="ascend_icon">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge" ><Abs value="3" /></Border>
                    <Border type="TopEdge" ><Abs value="3" /></Border>
                    <Border type="Width" ><PixelSize pixel_id="TaharezLook/HeaderBarSortUp" value="Width" /></Border>
                    <Border type="Height" ><PixelSize pixel_id="TaharezLook/HeaderBarSortUp" value="Height" /></Border>
                </Area>
                <Com pixel_id="TaharezLook/HeaderBarSortDown" />
            </ImageryCom>
        </StyleSection>
        <StyleSection id="descend_icon">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge" ><Abs value="3" /></Border>
                    <Border type="TopEdge" ><Abs value="3" /></Border>
                    <Border type="Width" ><PixelSize pixel_id="TaharezLook/HeaderBarSortDown" value="Width" /></Border>
                    <Border type="Height" ><PixelSize pixel_id="TaharezLook/HeaderBarSortDown" value="Height" /></Border>
                </Area>
                <Com pixel_id="TaharezLook/HeaderBarSortUp" />
            </ImageryCom>
        </StyleSection>
        <StyleSection id="label">
            <TextCom>
                <Area>
                    <Border type="LeftEdge" >
                        <Operator type="Add">
                            <PixelSize pixel_id="TaharezLook/HeaderBarSortUp" value="Width" />
                            <Abs value="5" />
                        </Operator>
                    </Border>
                </Area>
                <VLayout type="centre" />
            </TextCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="segment_normal" />
                <Section style_section="splitter_normal" />
                <Section style_section="label" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Hover">
            <Layer>
                <Section style_section="segment_hover" />
                <Section style_section="splitter_normal" />
                <Section style_section="label" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="SplitterHover">
            <Layer>
                <Section style_section="segment_normal" />
                <Section style_section="splitter_hover" />
                <Section style_section="label" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="DragState" clipped="false" >
            <Layer priority="1" >
                <Section style_section="segment_normal">
                    <Colours top_left="90FFFFFF" top_right="90FFFFFF" bottom_left="90FFFFFF" bottom_right="90FFFFFF" />
                </Section>
                <Section style_section="splitter_normal">
                    <Colours top_left="90FFFFFF" top_right="90FFFFFF" bottom_left="90FFFFFF" bottom_right="90FFFFFF" />
                </Section>
                <Section style_section="label">
                    <Colours top_left="90FFFFFF" top_right="90FFFFFF" bottom_left="90FFFFFF" bottom_right="90FFFFFF" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="AscIcon">
            <Layer>
                <Section style_section="ascend_icon" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="DescIcon">
            <Layer>
                <Section style_section="descend_icon" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="DragAscIcon" clipped="false">
            <Layer priority="1" >
                <Section style_section="ascend_icon">
                    <Colours top_left="90FFFFFF" top_right="90FFFFFF" bottom_left="90FFFFFF" bottom_right="90FFFFFF" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DragDescIcon" clipped="false">
            <Layer priority="1" >
                <Section style_section="descend_icon">
                    <Colours top_left="90FFFFFF" top_right="90FFFFFF" bottom_left="90FFFFFF" bottom_right="90FFFFFF" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="segment_normal">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
                <Section style_section="splitter_normal">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
                <Section style_section="label">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>

