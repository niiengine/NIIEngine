    <!--
    ***************************************************
        TaharezLook/Editbox
    ***************************************************
    -->
    <WidgetStyle id="TaharezLook/Editbox">
        <PropertyDefine id="NormalTextColour" value="FFFFFFFF" signal_refresh="true" type="ColourRect"/>
        <PropertyDefine id="SelectedTextColour" value="FF000000" signal_refresh="true" type="ColourRect"/>
        <PropertyDefine id="ActiveSelectionColour" value="FF607FFF" signal_refresh="true" type="ColourRect"/>
        <PropertyDefine id="InactiveSelectionColour" value="FF808080" signal_refresh="true" type="ColourRect"/>
        <Property name="MouseCursorImage" value="TaharezLook/MouseTextBar" />
        <Property name="Size" value="{{0, 128}, {0, 32}}" />
        <Special name="TextArea">
            <Area>
                <Border type="LeftEdge" ><Abs value="5" /></Border>
                <Border type="TopEdge" ><Abs value="5" /></Border>
                <Border type="RightEdge" ><Rel scale="1.0" offset="-5" value="RightEdge" /></Border>
                <Border type="BottomEdge" ><Rel scale="1.0" offset="-5" value="BottomEdge" /></Border>
            </Area>
        </Special>
        <StyleSection id="container_normal">
            <FrameCom>
                <Com type="LeftEdge" pixel_id="TaharezLook/EditBoxLeft" />
                <Com type="RightEdge" pixel_id="TaharezLook/EditBoxRight" />
                <Com type="Background" pixel_id="TaharezLook/EditBoxMiddle" />
            </FrameCom>
        </StyleSection>
        <StyleSection id="selection">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge" ><Abs value="0" /></Border>
                    <Border type="TopEdge" ><Abs value="0" /></Border>
                    <Border type="RightEdge" ><Rel scale="1.0" value="RightEdge" /></Border>
                    <Border type="BottomEdge" ><Rel scale="1.0" value="BottomEdge" /></Border>
                </Area>
                <Com pixel_id="TaharezLook/TextSelectionBrush" />
                <VLayout type="stretch" />
                <HLayout type="stretch" />
            </ImageryCom>
        </StyleSection>
        <StyleSection id="Caret">
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge" ><Abs value="0" /></Border>
                    <Border type="TopEdge" ><Abs value="0" /></Border>
                    <Border type="Width" ><PixelSize pixel_id="TaharezLook/EditBoxCaret" value="Width" /></Border>
                    <Border type="BottomEdge" ><Rel scale="1.0" value="BottomEdge" /></Border>
                </Area>
                <Com pixel_id="TaharezLook/EditBoxCaret" />
                <VLayout type="stretch" />
                <HLayout type="stretch" />
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="container_normal" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="ReadOnly">
            <Layer>
                <Section style_section="container_normal" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="container_normal">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="SelectColour">
            <Layer>
                <Section style_section="selection">
                    <ColourAttribute name="ActiveSelectionColour" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="UnselectColour">
            <Layer>
                <Section style_section="selection">
                    <ColourAttribute name="InactiveSelectionColour" />
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>

    <!--
    ***************************************************
        TaharezLook/Titlebar
    ***************************************************
    -->
    <WidgetStyle id="TaharezLook/Titlebar">
        <PropertyDefine id="CaptionColour" value="FFFFFFFF" signal_refresh="true" type="ColourRect"/>
        <Property name="NonClient" value="True" />
        <Property name="Size" value="{{0, 256}, {0, 32}}" />
        <StyleSection id="main">
            <ImageryCom>
                <Com pixel_id="TaharezLook/NewTitlebarLeft" />
                <VLayout type="stretch" />
            </ImageryCom>
            <ImageryCom>
                <Area>
                    <Border type="RightEdge" >
                        <Operator type="Subtract">
                            <Rel scale="1" value="RightEdge"/>
                            <Operator type="Add">
                                <PixelSize pixel_id="TaharezLook/SysAreaRight" value="Width"/>
                                <PixelSize pixel_id="TaharezLook/SysAreaMiddle" value="Width" />
                            </Operator>
                        </Operator>
                    </Border>
                </Area>
                <Com pixel_id="TaharezLook/NewTitlebarRight" />
                <VLayout type="stretch" />
                <HLayout type="right" />
            </ImageryCom>
            <ImageryCom>
                <Area>
                    <Border type="RightEdge" >
                        <Operator type="Subtract">
                            <Rel scale="1" value="RightEdge"/>
                            <PixelSize pixel_id="TaharezLook/SysAreaRight" value="Width" />
                        </Operator>
                    </Border>
                </Area>
                <Com pixel_id="TaharezLook/SysAreaMiddle" />
                <VLayout type="stretch" />
                <HLayout type="right" />
            </ImageryCom>
            <ImageryCom>
                <Com pixel_id="TaharezLook/SysAreaRight" />
                <VLayout type="stretch" />
                <HLayout type="right" />
            </ImageryCom>
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge" ><PixelSize pixel_id="TaharezLook/NewTitlebarLeft" value="Width" /></Border>
                    <Border type="RightEdge" >
                        <Operator type="Subtract">
                            <Operator type="Subtract">
                                <Operator type="Subtract">
                                    <Rel scale="1" value="RightEdge"/>
                                    <PixelSize pixel_id="TaharezLook/SysAreaRight" value="Width"/>
                                </Operator>
                                <PixelSize pixel_id="TaharezLook/SysAreaMiddle" value="Width"/>
                            </Operator>
                            <PixelSize pixel_id="TaharezLook/NewTitlebarRight" value="Width" />
                        </Operator>
                    </Border>
                </Area>
                <Com pixel_id="TaharezLook/NewTitlebarMiddle" />
                <VLayout type="stretch" />
                <HLayout type="stretch" />
            </ImageryCom>
        </StyleSection>
        <StyleSection id="caption">
            <TextCom>
                <Area>
                    <Border type="LeftEdge" ><PixelSize pixel_id="TaharezLook/NewTitlebarLeft" value="Width" /></Border>
                    <Border type="Width" ><Rel scale="1" offset="-75" value="Width" /></Border>
                </Area>
                <ColourAttribute name="CaptionColour" />
                <VLayout type="centre" />
            </TextCom>
        </StyleSection>
        <StyleLayout name="Active">
            <Layer>
                <Section style_section="main" />
                <Section style_section="caption" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Inactive">
            <Layer>
                <Section style_section="main" />
                <Section style_section="caption" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="main">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
                <Section style_section="caption">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>

    <!--
    ***************************************************
        TaharezLook/FrameWindow
    ***************************************************
    -->
    <WidgetStyle id="TaharezLook/FrameWindow">
        <PropertyDefine id="ClientAreaColour" value="FF141B38" signal_refresh="true" type="ColourRect"/>
        <PropertyLinkDefinition name="CaptionColour" widget="__auto_titlebar__" targetProperty="CaptionColour" value="FFFFFFFF" type="ColourRect"/>
        <PropertyLinkDefinition name="TitlebarFont" widget="__auto_titlebar__" targetProperty="Font" type="Font"/>
        <Property name="NSSizingCursorImage" value="TaharezLook/MouseNoSoCursor" />
        <Property name="EWSizingCursorImage" value="TaharezLook/MouseEsWeCursor" />
        <Property name="NWSESizingCursorImage" value="TaharezLook/MouseNwSeCursor" />
        <Property name="NESWSizingCursorImage" value="TaharezLook/MouseNeSwCursor" />
        <Property name="MinSize" value="{{0.1,0},{0.1,0}}" />
        <Property name="AutoRenderingSurface" value="True" />
        <Property name="Size" value="{{0.3, 0}, {0.3, 0}}" />
        <Special name="ClientWithTitleWithFrame">
            <Area>
                <Border type="LeftEdge" ><PixelSize pixel_id="TaharezLook/WindowTopLeft" value="Width" /></Border>
                <Border type="TopEdge" ><WidgetSize widget="__auto_titlebar__" value="BottomEdge" /></Border>
                <Border type="RightEdge" >
                    <Operator type="Subtract">
                        <Rel scale="1" value="RightEdge" />
                        <PixelSize pixel_id="TaharezLook/WindowTopRight" value="Width" />
                    </Operator>
                </Border>
                <Border type="BottomEdge" >
                    <Operator type="Subtract">
                        <Rel scale="1" value="BottomEdge" />
                        <PixelSize pixel_id="TaharezLook/WindowBottomEdge" value="Height" />
                    </Operator>
                </Border>
            </Area>
        </Special>
        <Special name="ClientWithTitleNoFrame">
            <Area>
                <Border type="LeftEdge" ><Abs value="0" /></Border>
                <Border type="TopEdge" ><WidgetSize widget="__auto_titlebar__" value="BottomEdge" /></Border>
                <Border type="Width" ><Rel scale="1" value="Width" /></Border>
                <Border type="BottomEdge" ><WidgetSize value="BottomEdge" /></Border>
            </Area>
        </Special>
        <Special name="ClientNoTitleWithFrame">
            <Area>
                <Border type="LeftEdge" ><PixelSize pixel_id="TaharezLook/WindowTopLeft" value="Width" /></Border>
                <Border type="TopEdge" ><PixelSize pixel_id="TaharezLook/WindowTopEdge" value="Height" /></Border>
                <Border type="RightEdge" >
                    <Operator type="Subtract">
                        <Rel scale="1" value="RightEdge" />
                        <PixelSize pixel_id="TaharezLook/WindowTopRight" value="Width" />
                    </Operator>
                </Border>
                <Border type="BottomEdge" >
                    <Operator type="Subtract">
                        <Rel scale="1" value="BottomEdge" />
                        <PixelSize pixel_id="TaharezLook/WindowBottomEdge" value="Height" />
                    </Operator>
                </Border>
            </Area>
        </Special>
        <Special name="ClientNoTitleNoFrame" />
        <Widget style_id="TaharezLook/Titlebar" id="__auto_titlebar__">
            <Area>
                <Border type="Width" ><Rel scale="1" value="Width" /></Border>
                <Border type="Height" ><FontDim type="LineSpacing" padding="8" /></Border>
            </Area>
            <Property name="AlwaysOnTop" value="False" />
        </Widget>
        <Widget style_id="TaharezLook/ImageButton" id="__auto_closebutton__">
            <Area>
                <Border type="LeftEdge" >
                    <Operator type="Subtract">
                        <Operator type="Subtract">
                            <Rel scale="1" value="LeftEdge"/>
                            <PixelSize pixel_id="TaharezLook/SysAreaRight" value="Width"/>
                        </Operator>
                        <PixelSize pixel_id="TaharezLook/SysAreaMiddle" value="Width" />
                    </Operator>
                </Border>
                <Border type="TopEdge" >
                    <Operator type="Multiply">
                        <Operator type="Subtract">
                            <WidgetSize widget="__auto_titlebar__" value="Height" />
