                <Border type="Height" >
                    <Operator type="Multiply">
                        <Rel scale="0.6" value="Width" />
                        <Operator type="Divide">
                            <PixelSize pixel_id="TaharezLook/VertScrollUpNormal" value="Width" />
                            <PixelSize pixel_id="TaharezLook/VertScrollUpNormal" value="Height"/>
                        </Operator>
                    </Operator>
                </Border>
            </Area>
            <Property name="NormalImage" value="TaharezLook/VertScrollUpNormal" />
            <Property name="HoverImage" value="TaharezLook/VertScrollUpHover" />
            <Property name="PushedImage" value="TaharezLook/VertScrollUpNormal" />
        </Widget>
        <Widget  style_id="TaharezLook/LargeVerticalScrollbarThumb" id="__auto_thumb__">
            <Area>
                <Border type="Width" ><Rel scale="0.4" value="Width" /></Border>
                <Border type="Height" >
                    <Operator type="Multiply">
                        <Rel scale="0.4" value="Width" />
                        <Operator type="Divide">
                            <PixelSize pixel_id="TaharezLook/VertScrollThumbNormal" value="Height" />
                            <PixelSize pixel_id="TaharezLook/VertScrollThumbNormal" value="Width"/>
                        </Operator>
                    </Operator>
                </Border>
            </Area>
        </Widget>
        <StyleSection id="main">
            <FrameCom>
                <Com type="TopEdge" pixel_id="TaharezLook/VertScrollTop" />
                <Com type="BottomEdge" pixel_id="TaharezLook/VertScrollBottom" />
                <Com type="Background" pixel_id="TaharezLook/VertScrollMiddle" />
            </FrameCom>
            <ImageryCom>
                <Area>
                    <Border type="LeftEdge" ><Rel scale="0.45" value="LeftEdge" /></Border>
                    <Border type="TopEdge" >
                        <Operator type="Add">
                            <WidgetSize widget="__auto_decbtn__" value="Height" />
                            <Operator type="Multiply">
                                <PixelSize pixel_id="TaharezLook/VertScrollTop" value="Height" />
                                <Abs value="0.5" />
                            </Operator>
                        </Operator>
                    </Border>
                    <Border type="RightEdge" ><Rel scale="1.0" value="RightEdge" /></Border>
                    <Border type="BottomEdge" >
                        <Operator type="Subtract">
                            <Rel scale="1.0" value="BottomEdge" />
                            <Operator type="Add">
                                <WidgetSize widget="__auto_decbtn__" value="Height" />
                                <Operator type="Multiply">
                                    <PixelSize pixel_id="TaharezLook/VertScrollTop" value="Height" />
                                    <Abs value="0.5" />
                                </Operator>
                            </Operator>
                        </Operator>
                    </Border>
                </Area>
                <Com pixel_id="TaharezLook/VertScrollBarSegment" />
                <VLayout type="tile" />
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="main" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="main">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>

