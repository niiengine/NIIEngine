    <!--
    ***************************************************
        TaharezLook/ListboxItem
    ***************************************************
    -->
    <WidgetStyle id="TaharezLook/ListboxItem">
        <PropertyDefine id="TextColour" value="FFFFFFFF" signal_refresh="true" type="ColourRect"/>
        <PropertyDefine id="SelectedTextColour" value="FFFFFFFF" signal_refresh="true" type="ColourRect"/>
        <PropertyDefine id="SelectionBrush" value="TaharezLook/ListboxSelectionBrush" signal_refresh="true" type="Pixel"/>
        <PropertyDefine id="SelectionColour" value="FF4444AA" signal_refresh="true" type="ColourRect"/>
        <Property name="Selectable" value="True" />
        <Property name="MouseInputPropagationEnabled" value="True" />
        <Property name="Size" value="{{0, 128}, {0, 32}}" />
        <Special name="ContentSize">
            <Area>
                <Border type="Width" >
                    <FontDim type="HorzExtent" padding="6" />
                </Border>
                <Border type="Height" >
                    <FontDim type="LineSpacing" />
                </Border>
            </Area>
        </Special>
        <StyleSection id="label">
            <TextCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="3" />
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" offset="-3" value="RightEdge" />
                    </Border>
                </Area>
            </TextCom>
        </StyleSection>
        <StyleSection id="selection">
            <ImageryCom>
                <ImageProperty name="SelectionBrush" />
                <ColourAttribute name="SelectionColour" />
                <VLayout type="stretch" />
                <HLayout type="stretch" />
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="label">
                    <ColourAttribute name="TextColour" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="label">
                    <ColourAttribute name="TextColour" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="SelectEnable">
            <Layer>
                <Section style_section="selection" />
                <Section style_section="label">
                    <ColourAttribute name="SelectedTextColour" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="SelectDisable">
            <Layer>
                <Section style_section="selection" />
                <Section style_section="label">
                    <ColourAttribute name="SelectedTextColour" />
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>

