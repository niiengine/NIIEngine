<?xml version="1.0" ?>
<Style ver="7">
    <!--
    ***************************************************
        TaharezLook/Label
    ***************************************************
    -->
    <WidgetStyle id="TaharezLook/Label">
        <PropertyDefine id="NormalTextColour" value="FFFFFFFF" signal_refresh="true" type="Colour" />
        <PropertyDefine id="DisabledTextColour" value="FFDDDDDD" signal_refresh="true" type="Colour" />
        <PropertyDefine id="VertFormatting" value="centre" signal_refresh="true" type="VerticalTextFormatting" />
        <PropertyDefine id="HorzFormatting" value="centre" signal_refresh="true" type="HorizontalTextFormatting" />
        <Property name="Size" value="{{0, 128}, {0, 32}}" />
        <StyleSection id="Label">
            <TextCom>
                <Area>
	            <Border type="LeftEdge"><Abs value="0" /></Border>
	            <Border type="TopEdge"><Abs value="0" /></Border>
	            <Border type="Width"><Rel scale="1" value="Width" /></Border>
	            <Border type="Height"><Rel scale="1" value="Height" /></Border>
                </Area>
                <VertFormatProperty name="VertFormatting" />
                <HorzFormatProperty name="HorzFormatting" />
            </TextCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="Label">
                    <ColourAttribute name="NormalTextColour" />
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="Label">
                    <ColourAttribute name="DisabledTextColour" />
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>

