-->
    <WidgetStyle id="SampleBrowserSkin/ProgressBar">
        <Special name="ProgressArea">
            <Area>
                <Border type="LeftEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="TopEdge">
                    <Abs value="0"/>
                </Border>
                <Border type="RightEdge">
                    <Rel scale="1" value="RightEdge"/>
                </Border>
                <Border type="BottomEdge">
                    <Rel scale="1" value="BottomEdge"/>
                </Border>
            </Area>
        </Special>
        <StyleSection id="frame">
            <FrameCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="Width">
                        <Rel scale="1" value="Width"/>
                    </Border>
                    <Border type="Height">
                        <Rel scale="1" value="Height"/>
                    </Border>
                </Area>
                <Com type="TopLeftCorner" pixel_id="SampleBrowserSkin/ProgessBarTopLeft"/>
                <Com type="TopRightCorner" pixel_id="SampleBrowserSkin/ProgessBarTopRight"/>
                <Com type="BottomLeftCorner" pixel_id="SampleBrowserSkin/ProgessBarBotLeft"/>
                <Com type="BottomRightCorner" pixel_id="SampleBrowserSkin/ProgessBarBotRight"/>
                <Com type="LeftEdge" pixel_id="SampleBrowserSkin/ProgessBarLeft"/>
                <Com type="TopEdge" pixel_id="SampleBrowserSkin/ProgessBarTop"/>
                <Com type="RightEdge" pixel_id="SampleBrowserSkin/ProgessBarRight"/>
                <Com type="BottomEdge" pixel_id="SampleBrowserSkin/ProgessBarBot"/>
                <Com type="Background" pixel_id="SampleBrowserSkin/ProgessBarMiddle"/>
            </FrameCom>
        </StyleSection>
        <StyleSection id="progress">
            <FrameCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="Width">
                        <Operator type="Multiply">
                            <Rel scale="1" value="Width"/>
                            <PropertyDim name="CurrentProgress"/>
                        </Operator>
                    </Border>
                    <Border type="Height">
                        <Rel scale="1" value="Height"/>
                    </Border>
                </Area>
                <Com type="TopLeftCorner" pixel_id="SampleBrowserSkin/ProgessBarOverTopLeft"/>
                <Com type="TopRightCorner" pixel_id="SampleBrowserSkin/ProgessBarOverTopRight"/>
                <Com type="BottomLeftCorner" pixel_id="SampleBrowserSkin/ProgessBarOverBotLeft"/>
