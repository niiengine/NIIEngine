                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Com type="TopLeftCorner" pixel_id="WindowsLook/TooltipTopLeft"/>
                <Com type="TopRightCorner" pixel_id="WindowsLook/TooltipTopRight"/>
                <Com type="BottomLeftCorner" pixel_id="WindowsLook/TooltipBottomLeft"/>
                <Com type="BottomRightCorner" pixel_id="WindowsLook/TooltipBottomRight"/>
                <Com type="LeftEdge" pixel_id="WindowsLook/TooltipLeftEdge"/>
                <Com type="TopEdge" pixel_id="WindowsLook/TooltipTopEdge"/>
                <Com type="RightEdge" pixel_id="WindowsLook/TooltipRightEdge"/>
                <Com type="BottomEdge" pixel_id="WindowsLook/TooltipBottomEdge"/>
                <Com type="Background" pixel_id="WindowsLook/TooltipMiddle"/>
            </FrameCom>
        </StyleSection>
        <StyleSection id="label">
            <TextCom>
                <Area>
                    <Border type="LeftEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="TopEdge">
                        <Abs value="0"/>
                    </Border>
                    <Border type="RightEdge">
                        <Rel scale="1" value="RightEdge"/>
                    </Border>
                    <Border type="BottomEdge">
                        <Rel scale="1" value="BottomEdge"/>
                    </Border>
                </Area>
                <Colours top_left="FF000000" bottom_left="FF000000" top_right="FF000000" bottom_right="FF000000"/>
                <VLayout type="centre"/>
                <HLayout type="centre"/>
            </TextCom>
        </StyleSection>
        <StyleLayout name="Enable">
            <Layer>
                <Section style_section="main"/>
                <Section style_section="label"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disable">
            <Layer>
                <Section style_section="main"/>
                <Section style_section="label"/>
            </Layer>
        </StyleLayout>
        <AnimationDefinition duration="0.33" replayMode="once" name="FadeIn">
            <PropertyIDKeyFrameTrack interpolator="float" property="Alpha">
                <KeyFrame position="0" sourceProperty="Alpha"/>
                <KeyFrame position="0.33" progression="linear" value="1"/>
            </PropertyIDKeyFrameTrack>
            <Subscription action="Start" event="TooltipActive"/>
        </AnimationDefinition>
        <AnimationDefinition duration="0.33" replayMode="once" name="FadeOut">
            <PropertyIDKeyFrameTrack interpolator="float" property="Alpha">
                <KeyFrame position="0" sourceProperty="Alpha"/>
                <KeyFrame position="0.33" progression="linear" value="0"/>
            </PropertyIDKeyFrameTrack>
            <PropertyIDKeyFrameTrack interpolator="bool" property="Visible">
                <KeyFrame position="0" sourceProperty="Visible"/>
                <KeyFrame position="0.33" progression="discrete" value="false"/>
            </PropertyIDKeyFrameTrack>
            <Subscription action="Start" event="TooltipInactive"/>
        </AnimationDefinition>
    </WidgetStyle>
</Style>
