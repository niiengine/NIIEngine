    <WidgetStyle name="Vanilla/Tooltip">
    <Property name="Size" value="{{0, 128}, {0, 32}}" />
        <StyleSpecial name="TextArea">
            <ComArea>
                <SizeDefine type="LeftEdge" ><PixelSize name="Vanilla-Images/TooltipLeftEdge" dimension="Width" /></SizeDefine>
                <SizeDefine type="TopEdge" ><PixelSize name="Vanilla-Images/TooltipTopEdge" dimension="Height" /></SizeDefine>
                <SizeDefine type="RightEdge" >
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="RightEdge" />
                        <PixelSize name="Vanilla-Images/TooltipRightEdge" dimension="Width" />
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="BottomEdge" >
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="BottomEdge" />
                        <PixelSize name="Vanilla-Images/TooltipBottomEdge" dimension="Height" />
                    </SizeOp>
                </SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleSection name="main">
            <FrameCom>
                <ComPixel type="TopLeftCorner" pixel_id="Vanilla-Images/TooltipTopLeft" />
                <ComPixel type="TopRightCorner" pixel_id="Vanilla-Images/TooltipTopRight" />
                <ComPixel type="BottomLeftCorner" pixel_id="Vanilla-Images/TooltipBottomLeft" />
                <ComPixel type="BottomRightCorner" pixel_id="Vanilla-Images/TooltipBottomRight" />
                <ComPixel type="LeftEdge" pixel_id="Vanilla-Images/TooltipLeftEdge" />
                <ComPixel type="RightEdge" pixel_id="Vanilla-Images/TooltipRightEdge" />
                <ComPixel type="TopEdge" pixel_id="Vanilla-Images/TooltipTopEdge" />
                <ComPixel type="BottomEdge" pixel_id="Vanilla-Images/TooltipBottomEdge" />
                <ComPixel type="Background" pixel_id="Vanilla-Images/TooltipMiddle" />
            </FrameCom>
        </StyleSection>
        <StyleSection name="label">
            <TextCom>
                <ComArea>
                    <SizeDefine type="LeftEdge" ><PixelSize name="Vanilla-Images/TooltipLeftEdge" dimension="Width" /></SizeDefine>
                    <SizeDefine type="TopEdge" ><PixelSize name="Vanilla-Images/TooltipTopEdge" dimension="Height" /></SizeDefine>
                    <SizeDefine type="RightEdge" >
                        <SizeOp op="Subtract">
                            <RelSize scale="1" type="RightEdge" />
                            <PixelSize name="Vanilla-Images/TooltipRightEdge" dimension="Width" />
                        </SizeOp>
                    </SizeDefine>
                    <SizeDefine type="BottomEdge" >
                        <SizeOp op="Subtract">
                            <RelSize scale="1" type="BottomEdge" />
                            <PixelSize name="Vanilla-Images/TooltipBottomEdge" dimension="Height" />
                        </SizeOp>
                    </SizeDefine>
                </ComArea>
                <Colours top_left="FF000000" top_right="FF000000" bottom_left="FF000000" bottom_right="FF000000" />
                <VLayout type="Centre" />
                <HLayout type="Centre" />
            </TextCom>
        </StyleSection>
        <StyleLayout name="Enabled">
            <Layer>
                <Section id="main" />
                <Section id="label" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disabled">
            <Layer>
                <Section id="main" />
                <Section id="label" />
            </Layer>
        </StyleLayout>
        
        <Animation name="FadeIn" duration="0.33" play_mode="once">
            <Track property="Alpha" data_type="float">
                <KeyFrame position="0" property_value="Alpha" />
                <KeyFrame position="0.33" value="1" calc_mode="constant" />
            </Track>
            
            <Event event="TooltipActive" action="Start" />
        </Animation>
        
        <Animation name="FadeOut" duration="0.33" play_mode="once">
            <Track property="Alpha" data_type="float">
                <KeyFrame position="0" property_value="Alpha" />
                <KeyFrame position="0.33" value="0" calc_mode="constant" />
            </Track>
            <Track property="Visible" data_type="bool">
                <KeyFrame position="0" property_value="Visible" />
                <KeyFrame position="0.33" value="false" calc_mode="point" />
            </Track>
            
            <Event event="TooltipInactive" action="Start" />
        </Animation>
    </WidgetStyle>
