<?xml version="1.0" encoding="UTF-8"?>
<!--
    OgreTray.looknfeel by Paul D. Turner

    This looknfeel specifies a set of WidgetStyle definitions that are intended
    to recreate the overall widget_style of Ogre's new SDK tray system within CEGUI.

    This looknfeel has been made for use in some upcoming recreations of the
    Ogre samples, that use CEGUI instead of the new Ogre SDK tray system.  This
    is being done mostly for fun, though also to demonstrate the use of CEGUI
    with Ogre.
-->
<Style ver="7">
    <WidgetStyle name="OgreTray/Label">
        <StyleProperty signal_refresh="true" value="FF000000" type="Colour" name="NormalTextColour"/>
        <StyleProperty signal_refresh="true" value="FF444444" type="Colour" name="DisabledTextColour"/>
        <StyleProperty signal_refresh="true" value="Centre" type="VTextLayout" name="VTLayout1"/>
        <StyleProperty signal_refresh="true" value="Centre" type="HTextLayout" name="HTLayout1"/>
        <Property name="Size" value="{{0, 128}, {0, 32}}" />
        <StyleSection name="Label">
            <TextCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="Width">
                        <RelSize scale="1" type="Width"/>
                    </SizeDefine>
                    <SizeDefine type="Height">
                        <RelSize scale="1" type="Height"/>
                    </SizeDefine>
                </ComArea>
                <VLayoutProperty name="VTLayout1"/>
                <HLayoutProperty name="HTLayout1"/>
            </TextCom>
        </StyleSection>
        <StyleLayout name="Enabled">
            <Layer>
                <Section id="Label">
                    <ColourProperty name="NormalTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disabled">
            <Layer>
                <Section id="Label">
                    <ColourProperty name="DisabledTextColour"/>
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <WidgetStyle name="OgreTray/BGTest">
        <Property name="Size" value="{{0, 128}, {0, 128}}" />
        <StyleSection name="main">
            <ImageryCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="RightEdge">
                        <RelSize scale="1" type="RightEdge"/>
                    </SizeDefine>
                    <SizeDefine type="BottomEdge">
                        <RelSize scale="1" type="BottomEdge"/>
                    </SizeDefine>
                </ComArea>
                <ComPixel pixel_id="OgreTrayImages/BandsFull"/>
                <VLayout type="Extrude"/>
                <HLayout type="Extrude"/>
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enabled">
            <Layer>
                <Section id="main">
                    <Colours top_left="FF779988" bottom_left="FF779988" top_right="FF779988" bottom_right="FF779988"/>
                </Section>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disabled">
            <Layer>
                <Section id="main"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Titlebar
-->
    <WidgetStyle name="OgreTray/Titlebar">
        <StyleProperty signal_refresh="true" value="FFe5ffb2" type="ColourArea" name="CaptionColour"/>
        <StyleProperty value="Centre" type="VTextLayout" name="VTLayout1"/>
        <StyleProperty value="Centre" type="HTextLayout" name="HTLayout1"/>
        <Property name="NonClient" value="true"/>
        <Property name="Size" value="{{0, 256}, {0, 32}}" />
        <StyleSection name="normal">
            <FrameCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="RightEdge">
                        <RelSize scale="1" type="RightEdge"/>
                    </SizeDefine>
                    <SizeDefine type="BottomEdge">
                        <RelSize scale="1" type="BottomEdge"/>
                    </SizeDefine>
                </ComArea>
                <ComPixel type="LeftEdge" pixel_id="OgreTrayImages/CaptionL"/>
                <ComPixel type="RightEdge" pixel_id="OgreTrayImages/CaptionR"/>
                <ComPixel type="Background" pixel_id="OgreTrayImages/CaptionM"/>
            </FrameCom>
        </StyleSection>
        <StyleSection name="caption">
            <TextCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="RightEdge">
                        <RelSize scale="1" type="RightEdge"/>
                    </SizeDefine>
                    <SizeDefine type="BottomEdge">
                        <RelSize scale="1" type="BottomEdge"/>
                    </SizeDefine>
                </ComArea>
                <ColourProperty name="CaptionColour"/>
                <VLayoutProperty name="VTLayout1"/>
                <HLayoutProperty name="HTLayout1"/>
            </TextCom>
        </StyleSection>
        <StyleLayout name="Active">
            <Layer>
                <Section id="normal"/>
                <Section id="caption"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Inactive">
            <Layer>
                <Section id="normal"/>
                <Section id="caption"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disabled">
            <Layer>
                <Section id="normal"/>
                <Section id="caption"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Frame Window
-->
    <WidgetStyle name="OgreTray/FrameWindow">
        <ListenerProperty target="CaptionColour" widget="__auto_titlebar__" value="FFe5ffb2" type="ColourArea" name="CaptionColour"/>
        <ListenerProperty target="Font" widget="__auto_titlebar__" type="Font" name="CaptionFont"/>
        <Property name="NSSizingCursorImage" value="OgreTrayImages/MouseArrow"/>
        <Property name="EWSizingCursorImage" value="OgreTrayImages/MouseArrow"/>
        <Property name="NWSESizingCursorImage" value="OgreTrayImages/MouseArrow"/>
        <Property name="NESWSizingCursorImage" value="OgreTrayImages/MouseArrow"/>
        <Property name="AutoRenderingSurface" value="true"/>
        <Property name="Size" value="{{0.3, 0}, {0.3, 0}}" />
        <StyleSpecial name="ClientWithTitleWithFrame">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <SizeOp op="Divide">
                        <PixelSize name="OgreTrayImages/TrayL" dimension="Width"/>
                        <AbsSize value="2"/>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <SizeOp op="Add">
                        <WidgetSize widget="__auto_titlebar__" dimension="BottomEdge"/>
                        <SizeOp op="Divide">
                            <PixelSize name="OgreTrayImages/TrayT" dimension="Height"/>
                            <AbsSize value="2"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="RightEdge">
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="RightEdge"/>
                        <SizeOp op="Divide">
                            <PixelSize name="OgreTrayImages/TrayR" dimension="Width"/>
                            <AbsSize value="2"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="BottomEdge">
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="BottomEdge"/>
                        <SizeOp op="Divide">
                            <PixelSize name="OgreTrayImages/TrayB" dimension="Height"/>
                            <AbsSize value="2"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleSpecial name="ClientWithTitleNoFrame">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <WidgetSize widget="__auto_titlebar__" dimension="BottomEdge"/>
                </SizeDefine>
                <SizeDefine type="Width">
                    <RelSize scale="1" type="Width"/>
                </SizeDefine>
                <SizeDefine type="BottomEdge">
                    <WidgetSize dimension="BottomEdge"/>
                </SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleSpecial name="ClientNoTitleWithFrame">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <SizeOp op="Divide">
                        <PixelSize name="OgreTrayImages/TrayL" dimension="Width"/>
                        <AbsSize value="2"/>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <SizeOp op="Divide">
                        <PixelSize name="OgreTrayImages/TrayT" dimension="Height"/>
                        <AbsSize value="2"/>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="RightEdge">
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="RightEdge"/>
                        <SizeOp op="Divide">
                            <PixelSize name="OgreTrayImages/TrayR" dimension="Width"/>
                            <AbsSize value="2"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="BottomEdge">
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="BottomEdge"/>
                        <SizeOp op="Divide">
                            <PixelSize name="OgreTrayImages/TrayB" dimension="Height"/>
                            <AbsSize value="2"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleSpecial name="ClientNoTitleNoFrame">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="Width">
                    <RelSize scale="1" type="Width"/>
                </SizeDefine>
                <SizeDefine type="Height">
                    <RelSize scale="1" type="Height"/>
                </SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleWidget id="__auto_titlebar__" type="OgreTray/Titlebar">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <SizeOp op="Divide">
                        <PixelSize name="OgreTrayImages/TrayL" dimension="Width"/>
                        <AbsSize value="2"/>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <SizeOp op="Divide">
                        <PixelSize name="OgreTrayImages/TrayT" dimension="Height"/>
                        <AbsSize value="2"/>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="Width">
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="Width"/>
                        <PixelSize name="OgreTrayImages/TrayL" dimension="Width"/>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="Height">
                    <FontSize padding="8" type="LineSpacing"/>
                </SizeDefine>
            </ComArea>
            <Property name="AlwaysOnTop" value="false"/>
        </StyleWidget>
        <!-- here we basically nullify the close button, for now at least -->
        <StyleWidget id="__auto_closebutton__" type="DefaultWindow">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="Width">
                    <RelSize scale="0" type="Width"/>
                </SizeDefine>
                <SizeDefine type="Height">
                    <RelSize scale="0" type="Height"/>
                </SizeDefine>
            </ComArea>
            <Property name="NonClient" value="true"/>
        </StyleWidget>
        <StyleSection name="frame">
            <FrameCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="RightEdge">
                        <RelSize scale="1" type="RightEdge"/>
                    </SizeDefine>
                    <SizeDefine type="BottomEdge">
                        <RelSize scale="1" type="BottomEdge"/>
                    </SizeDefine>
                </ComArea>
                <ComPixel type="TopLeftCorner" pixel_id="OgreTrayImages/TrayTL"/>
                <ComPixel type="TopRightCorner" pixel_id="OgreTrayImages/TrayTR"/>
                <ComPixel type="BottomLeftCorner" pixel_id="OgreTrayImages/TrayBL"/>
                <ComPixel type="BottomRightCorner" pixel_id="OgreTrayImages/TrayBR"/>
                <ComPixel type="LeftEdge" pixel_id="OgreTrayImages/TrayL"/>
                <ComPixel type="TopEdge" pixel_id="OgreTrayImages/TrayT"/>
                <ComPixel type="RightEdge" pixel_id="OgreTrayImages/TrayR"/>
                <ComPixel type="BottomEdge" pixel_id="OgreTrayImages/TrayB"/>
                <ComPixel type="Background" pixel_id="OgreTrayImages/TrayM"/>
            </FrameCom>
        </StyleSection>
        <StyleSection name="noframe">
            <ImageryCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="Width">
                        <RelSize scale="1" type="Width"/>
                    </SizeDefine>
                    <SizeDefine type="BottomEdge">
                        <RelSize scale="1" type="BottomEdge"/>
                    </SizeDefine>
                </ComArea>
                <ComPixel pixel_id="OgreTrayImages/TrayM"/>
                <VLayout type="Extrude"/>
                <HLayout type="Extrude"/>
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="ActiveWithTitleWithFrame">
            <Layer>
                <Section id="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="InactiveWithTitleWithFrame">
            <Layer>
                <Section id="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DisabledWithTitleWithFrame">
            <Layer>
                <Section id="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="ActiveWithTitleNoFrame">
            <Layer>
                <Section id="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="InactiveWithTitleNoFrame">
            <Layer>
                <Section id="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DisabledWithTitleNoFrame">
            <Layer>
                <Section id="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="ActiveNoTitleWithFrame">
            <Layer>
                <Section id="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="InactiveNoTitleWithFrame">
            <Layer>
                <Section id="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DisabledNoTitleWithFrame">
            <Layer>
                <Section id="frame"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="ActiveNoTitleNoFrame">
            <Layer>
                <Section id="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="InactiveNoTitleNoFrame">
            <Layer>
                <Section id="noframe"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="DisabledNoTitleNoFrame">
            <Layer>
                <Section id="noframe"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: A Thumb for all occasions - can be used by all widgets needing a thumb.
:::: (don't forget to specify free axis in <StyleWidget> definition!)
-->
    <WidgetStyle name="OgreTray/Thumb">
        <Property name="Size" value="{{0, 32}, {0, 32}}" />
        <StyleSection name="main">
            <ImageryCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="Width">
                        <RelSize scale="1" type="Width"/>
                    </SizeDefine>
                    <SizeDefine type="Height">
                        <RelSize scale="1" type="Height"/>
                    </SizeDefine>
                </ComArea>
                <ComPixel pixel_id="OgreTrayImages/Handle"/>
                <VLayout type="Extrude"/>
                <HLayout type="Extrude"/>
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Normal">
            <Layer>
                <Section id="main"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Hover">
            <Layer>
                <Section id="main"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Pushed">
            <Layer>
                <Section id="main"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
    <!--
:::: Vertical Scrollbar
-->
    <WidgetStyle name="OgreTray/VerticalScrollbar">
        <Property name="VerticalScrollbar" value="true"/>
        <Property name="WantsMultiClickEvents" value="false"/>
        <Property name="Size" value="{{0, 32}, {1, 0}}" />
        <StyleSpecial name="ThumbTrackArea">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="Width">
                    <RelSize scale="1" type="Width"/>
                </SizeDefine>
                <SizeDefine type="Height">
                    <RelSize scale="1" type="Height"/>
                </SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleWidget id="__auto_incbtn__" type="DefaultWindow">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="Width">
