    <!--
:::: Shared static widget components.
-->
    <WidgetStyle name="OgreTray/StaticShared">
        <StyleSection name="frame">
            <FrameCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <AbsSize value="0"/>
                    </SizeDefine>
                    <SizeDefine type="RightEdge">
                        <RelSize scale="1" type="RightEdge"/>
                    </SizeDefine>
                    <SizeDefine type="BottomEdge">
