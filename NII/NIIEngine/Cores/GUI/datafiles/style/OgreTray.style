                <SizeDefine type="TopEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="Width">
                    <RelSize scale="0" type="Width"/>
                </SizeDefine>
                <SizeDefine type="Height">
                    <RelSize scale="0" type="Width"/>
                </SizeDefine>
            </ComArea>
            <Property name="Visible" value="false"/>
        </StyleWidget>
        <StyleWidget id="__auto_thumb__" type="OgreTray/Thumb">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="Width">
                    <RelSize scale="1" type="Width"/>
                </SizeDefine>
                <SizeDefine type="Height">
                    <RelSize scale="1" type="Width"/>
                </SizeDefine>
            </ComArea>
            <Property name="VertFree" value="true"/>
        </StyleWidget>
        <StyleSection name="main">
            <FrameCom>
                <ComArea>
                    <SizeDefine type="LeftEdge">
                        <RelSize scale="0.11" type="Width"/>
                    </SizeDefine>
                    <SizeDefine type="TopEdge">
                        <RelSize scale="0.1" type="Width"/>
                    </SizeDefine>
                    <SizeDefine type="Width">
                        <RelSize scale="0.78" type="Width"/>
                    </SizeDefine>
                    <SizeDefine type="BottomEdge">
                        <SizeOp op="Subtract">
                            <RelSize scale="1" type="BottomEdge"/>
                            <RelSize scale="0.1" type="Width"/>
                        </SizeOp>
                    </SizeDefine>
                </ComArea>
                <ComPixel type="TopEdge" pixel_id="OgreTrayImages/VertTrackT"/>
                <ComPixel type="BottomEdge" pixel_id="OgreTrayImages/VertTrackB"/>
                <ComPixel type="Background" pixel_id="OgreTrayImages/VertTrackM"/>
            </FrameCom>
        </StyleSection>
        <StyleLayout name="Enabled">
            <Layer>
                <Section id="main"/>
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disabled">
            <Layer>
                <Section id="main"/>
            </Layer>
        </StyleLayout>
    </WidgetStyle>
