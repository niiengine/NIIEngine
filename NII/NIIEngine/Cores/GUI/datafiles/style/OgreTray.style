    <!--
:::: Tab control
-->
    <WidgetStyle name="OgreTray/TabControl">
        <Property name="TabButtonType" value="OgreTray/TabButton"/>
        <Property name="TabTextPadding" value="{0,6}"/>
        <StyleWidget id="__auto_TabPane__" type="OgreTray/TabContentPane">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="RightEdge">
                    <RelSize scale="1" type="Width"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <SizeOp op="Multiply">
                        <SizeProperty widget="__auto_TabPane__" name="EnableBottom"/>
                        <SizeProperty type="Height" name="TabHeight"/>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="Height">
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="Height"/>
                        <SizeProperty type="Height" name="TabHeight"/>
                    </SizeOp>
                </SizeDefine>
            </ComArea>
        </StyleWidget>
        <StyleWidget id="__auto_TabPane__Buttons" type="OgreTray/TabButtonPane">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="RightEdge">
                    <RelSize scale="1" type="Width"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <SizeOp op="Multiply">
                        <SizeProperty widget="__auto_TabPane__Buttons" name="EnableTop"/>
                        <SizeOp op="Subtract">
                            <RelSize scale="1" type="Height"/>
                            <SizeProperty type="Height" name="TabHeight"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="Height">
                    <SizeProperty type="Height" name="TabHeight"/>
                </SizeDefine>
            </ComArea>
        </StyleWidget>
        <StyleWidget id="__auto_TabPane__ScrollLeft" type="OgreTray/Button">
            <ComArea>
                <SizeDefine type="LeftEdge">
                    <AbsSize value="0"/>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <WidgetSize widget="__auto_TabPane__Buttons" dimension="TopEdge"/>
                </SizeDefine>
                <SizeDefine type="Width">
                    <SizeOp op="Multiply">
                        <WidgetSize widget="__auto_TabPane__Buttons" dimension="Height"/>
                        <SizeOp op="Divide">
                            <FontSize type="HorzExtent" string="&lt;&lt;"/>
                            <FontSize type="LineSpacing"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="BottomEdge">
                    <WidgetSize widget="__auto_TabPane__Buttons" dimension="BottomEdge"/>
                </SizeDefine>
            </ComArea>
            <Property name="AlwaysOnTop" value="true"/>
            <Property name="Text" value="&lt;&lt;"/>
        </StyleWidget>
        <StyleWidget id="__auto_TabPane__ScrollRight" type="OgreTray/Button">
            <ComArea>
                <SizeDefine type="Width">
                    <SizeOp op="Multiply">
                        <WidgetSize widget="__auto_TabPane__Buttons" dimension="Height"/>
                        <SizeOp op="Divide">
                            <FontSize type="HorzExtent" string="&gt;&gt;"/>
                            <FontSize type="LineSpacing"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="LeftEdge">
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="Width"/>
                        <WidgetSize widget="__auto_TabPane__ScrollRight" dimension="Width"/>
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="TopEdge">
                    <WidgetSize widget="__auto_TabPane__Buttons" dimension="TopEdge"/>
                </SizeDefine>
                <SizeDefine type="Height">
                    <WidgetSize widget="__auto_TabPane__Buttons" dimension="Height"/>
                </SizeDefine>
            </ComArea>
            <Property name="AlwaysOnTop" value="true"/>
            <Property name="Text" value="&gt;&gt;"/>
        </StyleWidget>
        <StyleLayout name="Enabled"/>
        <StyleLayout name="Disabled"/>
    </WidgetStyle>
