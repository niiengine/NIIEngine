                <ComPixel type="TopRightCorner" pixel_id="OgreTrayImages/TextBoxTR"/>
                <ComPixel type="BottomLeftCorner" pixel_id="OgreTrayImages/TextBoxBL"/>
                <ComPixel type="BottomRightCorner" pixel_id="OgreTrayImages/TextBoxBR"/>
                <ComPixel type="LeftEdge" pixel_id="OgreTrayImages/TextBoxL"/>
                <ComPixel type="TopEdge" pixel_id="OgreTrayImages/TextBoxT"/>
                <ComPixel type="RightEdge" pixel_id="OgreTrayImages/TextBoxR"/>
