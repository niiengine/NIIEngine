    <!--
    ***************************************************
        TaharezLook/Titlebar
    ***************************************************
    -->
    <WidgetStyle name="TaharezLook/Titlebar">
        <StyleProperty name="CaptionColour" value="FFFFFFFF" signal_refresh="true" type="ColourArea"/>
        <Property name="NonClient" value="true" />
        <Property name="Size" value="{{0, 256}, {0, 32}}" />
        <StyleSection name="main">
            <ImageryCom>
                <Pixel id="TaharezLook/NewTitlebarLeft" />
                <VLayout type="Extrude" />
            </ImageryCom>
            <ImageryCom>
                <ComArea>
                    <SizeDefine type="RightEdge" >
                        <SizeOp op="Subtract">
                            <RelSize scale="1" type="RightEdge"/>
                            <SizeOp op="Add">
                                <PixelSize name="TaharezLook/SysAreaRight" dimension="Width"/>
                                <PixelSize name="TaharezLook/SysAreaMiddle" dimension="Width" />
                            </SizeOp>
                        </SizeOp>
                    </SizeDefine>
                </ComArea>
                <Pixel id="TaharezLook/NewTitlebarRight" />
                <VLayout type="Extrude" />
                <HLayout type="RightAligned" />
            </ImageryCom>
            <ImageryCom>
                <ComArea>
                    <SizeDefine type="RightEdge" >
                        <SizeOp op="Subtract">
                            <RelSize scale="1" type="RightEdge"/>
                            <PixelSize name="TaharezLook/SysAreaRight" dimension="Width" />
                        </SizeOp>
                    </SizeDefine>
                </ComArea>
                <Pixel id="TaharezLook/SysAreaMiddle" />
                <VLayout type="Extrude" />
                <HLayout type="RightAligned" />
            </ImageryCom>
            <ImageryCom>
                <Pixel id="TaharezLook/SysAreaRight" />
                <VLayout type="Extrude" />
                <HLayout type="RightAligned" />
            </ImageryCom>
            <ImageryCom>
                <ComArea>
                    <SizeDefine type="LeftEdge" ><PixelSize name="TaharezLook/NewTitlebarLeft" dimension="Width" /></SizeDefine>
                    <SizeDefine type="RightEdge" >
                        <SizeOp op="Subtract">
                            <SizeOp op="Subtract">
                                <SizeOp op="Subtract">
                                    <RelSize scale="1" type="RightEdge"/>
                                    <PixelSize name="TaharezLook/SysAreaRight" dimension="Width"/>
                                </SizeOp>
                                <PixelSize name="TaharezLook/SysAreaMiddle" dimension="Width"/>
                            </SizeOp>
                            <PixelSize name="TaharezLook/NewTitlebarRight" dimension="Width" />
                        </SizeOp>
                    </SizeDefine>
                </ComArea>
                <Pixel id="TaharezLook/NewTitlebarMiddle" />
                <VLayout type="Extrude" />
                <HLayout type="Extrude" />
            </ImageryCom>
        </StyleSection>
        <StyleSection name="caption">
            <TextCom>
                <ComArea>
                    <SizeDefine type="LeftEdge" ><PixelSize name="TaharezLook/NewTitlebarLeft" dimension="Width" /></SizeDefine>
                    <SizeDefine type="Width" ><RelSize scale="1" offset="-75" type="Width" /></SizeDefine>
                </ComArea>
                <ColourProperty name="CaptionColour" />
                <VLayout type="Centre" />
            </TextCom>
        </StyleSection>
        <StyleLayout name="Active">
            <Layer>
                <Section id="main" />
                <Section id="caption" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Inactive">
            <Layer>
                <Section id="main" />
                <Section id="caption" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disabled">
            <Layer>
                <Section id="main">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
                <Section id="caption">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
            </Layer>
        </StyleLayout>
    </WidgetStyle>

    <!--
    ***************************************************
        TaharezLook/FrameWindow
    ***************************************************
    -->
    <WidgetStyle name="TaharezLook/FrameWindow">
        <StyleProperty name="ClientAreaColour" value="FF141B38" signal_refresh="true" type="ColourArea"/>
        <ListenerProperty name="CaptionColour" widget="__auto_titlebar__" target="CaptionColour" value="FFFFFFFF" type="ColourArea"/>
        <ListenerProperty name="TitlebarFont" widget="__auto_titlebar__" target="Font" type="Font"/>
        <Property name="NSSizingCursorImage" value="TaharezLook/MouseNoSoCursor" />
        <Property name="EWSizingCursorImage" value="TaharezLook/MouseEsWeCursor" />
        <Property name="NWSESizingCursorImage" value="TaharezLook/MouseNwSeCursor" />
        <Property name="NESWSizingCursorImage" value="TaharezLook/MouseNeSwCursor" />
        <Property name="MinSize" value="{{0.1,0},{0.1,0}}" />
        <Property name="AutoRenderingSurface" value="true" />
        <Property name="Size" value="{{0.3, 0}, {0.3, 0}}" />
        <StyleSpecial name="ClientWithTitleWithFrame">
            <ComArea>
                <SizeDefine type="LeftEdge" ><PixelSize name="TaharezLook/WindowTopLeft" dimension="Width" /></SizeDefine>
                <SizeDefine type="TopEdge" ><WidgetSize widget="__auto_titlebar__" dimension="BottomEdge" /></SizeDefine>
                <SizeDefine type="RightEdge" >
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="RightEdge" />
                        <PixelSize name="TaharezLook/WindowTopRight" dimension="Width" />
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="BottomEdge" >
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="BottomEdge" />
                        <PixelSize name="TaharezLook/WindowBottomEdge" dimension="Height" />
                    </SizeOp>
                </SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleSpecial name="ClientWithTitleNoFrame">
            <ComArea>
                <SizeDefine type="LeftEdge" ><AbsSize value="0" /></SizeDefine>
                <SizeDefine type="TopEdge" ><WidgetSize widget="__auto_titlebar__" dimension="BottomEdge" /></SizeDefine>
                <SizeDefine type="Width" ><RelSize scale="1" type="Width" /></SizeDefine>
                <SizeDefine type="BottomEdge" ><WidgetSize dimension="BottomEdge" /></SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleSpecial name="ClientNoTitleWithFrame">
            <ComArea>
                <SizeDefine type="LeftEdge" ><PixelSize name="TaharezLook/WindowTopLeft" dimension="Width" /></SizeDefine>
                <SizeDefine type="TopEdge" ><PixelSize name="TaharezLook/WindowTopEdge" dimension="Height" /></SizeDefine>
                <SizeDefine type="RightEdge" >
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="RightEdge" />
                        <PixelSize name="TaharezLook/WindowTopRight" dimension="Width" />
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="BottomEdge" >
                    <SizeOp op="Subtract">
                        <RelSize scale="1" type="BottomEdge" />
                        <PixelSize name="TaharezLook/WindowBottomEdge" dimension="Height" />
                    </SizeOp>
                </SizeDefine>
            </ComArea>
        </StyleSpecial>
        <StyleSpecial name="ClientNoTitleNoFrame" />
        <StyleWidget type="TaharezLook/Titlebar" nameSuffix="__auto_titlebar__">
            <ComArea>
                <SizeDefine type="Width" ><RelSize scale="1" type="Width" /></SizeDefine>
                <SizeDefine type="Height" ><FontSize type="LineSpacing" padding="8" /></SizeDefine>
            </ComArea>
            <Property name="AlwaysOnTop" value="false" />
        </StyleWidget>
        <StyleWidget type="TaharezLook/ImageButton" nameSuffix="__auto_closebutton__">
            <ComArea>
                <SizeDefine type="LeftEdge" >
                    <SizeOp op="Subtract">
                        <SizeOp op="Subtract">
                            <RelSize scale="1" type="LeftEdge"/>
                            <PixelSize name="TaharezLook/SysAreaRight" dimension="Width"/>
                        </SizeOp>
                        <PixelSize name="TaharezLook/SysAreaMiddle" dimension="Width" />
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="TopEdge" >
