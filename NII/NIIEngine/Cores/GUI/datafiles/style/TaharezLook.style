        <StyleProperty name="DisabledTextColour" value="FFDDDDDD" signal_refresh="true" type="Colour" />
        <StyleProperty name="VTLayout1" value="Centre" signal_refresh="true" type="VTextLayout" />
        <StyleProperty name="HTLayout1" value="Centre" signal_refresh="true" type="HTextLayout" />
        <Property name="Size" value="{{0, 128}, {0, 32}}" />
        <StyleSection name="Label">
            <TextCom>
                <ComArea>
	            <SizeDefine type="LeftEdge"><AbsSize value="0" /></SizeDefine>
	            <SizeDefine type="TopEdge"><AbsSize value="0" /></SizeDefine>
	            <SizeDefine type="Width"><RelSize scale="1" type="Width" /></SizeDefine>
	            <SizeDefine type="Height"><RelSize scale="1" type="Height" /></SizeDefine>
                </ComArea>
                <VLayoutProperty name="VTLayout1" />
                <HLayoutProperty name="HTLayout1" />
