            <Property name="HoverImage" value="TaharezLook/VertScrollDownHover" />
            <Property name="PushedImage" value="TaharezLook/VertScrollDownNormal" />
        </StyleWidget>
        <StyleWidget  type="TaharezLook/ImageButton" nameSuffix="__auto_decbtn__">
            <ComArea>
                <SizeDefine type="LeftEdge" ><RelSize scale="0.25" type="LeftEdge" /></SizeDefine>
                <SizeDefine type="TopEdge">
                    <SizeOp op="Multiply">
                        <PixelSize name="TaharezLook/VertScrollTop" dimension="Height" />
                        <AbsSize value="0.5" />
                    </SizeOp>
                </SizeDefine>
                <SizeDefine type="Width" ><RelSize scale="0.6" type="Width" /></SizeDefine>
                <SizeDefine type="Height" >
                    <SizeOp op="Multiply">
                        <RelSize scale="0.6" type="Width" />
                        <SizeOp op="Divide">
                            <PixelSize name="TaharezLook/VertScrollUpNormal" dimension="Width" />
                            <PixelSize name="TaharezLook/VertScrollUpNormal" dimension="Height"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
            </ComArea>
            <Property name="NormalImage" value="TaharezLook/VertScrollUpNormal" />
            <Property name="HoverImage" value="TaharezLook/VertScrollUpHover" />
            <Property name="PushedImage" value="TaharezLook/VertScrollUpNormal" />
        </StyleWidget>
        <StyleWidget  type="TaharezLook/LargeVerticalScrollbarThumb" nameSuffix="__auto_thumb__">
            <ComArea>
                <SizeDefine type="Width" ><RelSize scale="0.4" type="Width" /></SizeDefine>
                <SizeDefine type="Height" >
                    <SizeOp op="Multiply">
                        <RelSize scale="0.4" type="Width" />
                        <SizeOp op="Divide">
                            <PixelSize name="TaharezLook/VertScrollThumbNormal" dimension="Height" />
                            <PixelSize name="TaharezLook/VertScrollThumbNormal" dimension="Width"/>
                        </SizeOp>
                    </SizeOp>
                </SizeDefine>
            </ComArea>
        </StyleWidget>
        <StyleSection name="main">
            <FrameCom>
                <ComPixel type="TopEdge" pixel_id="TaharezLook/VertScrollTop" />
                <ComPixel type="BottomEdge" pixel_id="TaharezLook/VertScrollBottom" />
                <ComPixel type="Background" pixel_id="TaharezLook/VertScrollMiddle" />
            </FrameCom>
            <ImageryCom>
                <ComArea>
                    <SizeDefine type="LeftEdge" ><RelSize scale="0.45" type="LeftEdge" /></SizeDefine>
                    <SizeDefine type="TopEdge" >
                        <SizeOp op="Add">
                            <WidgetSize widget="__auto_decbtn__" dimension="Height" />
                            <SizeOp op="Multiply">
                                <PixelSize name="TaharezLook/VertScrollTop" dimension="Height" />
                                <AbsSize value="0.5" />
                            </SizeOp>
                        </SizeOp>
                    </SizeDefine>
                    <SizeDefine type="RightEdge" ><RelSize scale="1.0" type="RightEdge" /></SizeDefine>
                    <SizeDefine type="BottomEdge" >
                        <SizeOp op="Subtract">
                            <RelSize scale="1.0" type="BottomEdge" />
                            <SizeOp op="Add">
                                <WidgetSize widget="__auto_decbtn__" dimension="Height" />
                                <SizeOp op="Multiply">
                                    <PixelSize name="TaharezLook/VertScrollTop" dimension="Height" />
                                    <AbsSize value="0.5" />
                                </SizeOp>
                            </SizeOp>
                        </SizeOp>
                    </SizeDefine>
                </ComArea>
                <Pixel id="TaharezLook/VertScrollBarSegment" />
                <VLayout type="Tiled" />
            </ImageryCom>
        </StyleSection>
        <StyleLayout name="Enabled">
            <Layer>
                <Section id="main" />
            </Layer>
        </StyleLayout>
        <StyleLayout name="Disabled">
            <Layer>
                <Section id="main">
                    <Colours top_left="FF7F7F7F" top_right="FF7F7F7F" bottom_left="FF7F7F7F" bottom_right="FF7F7F7F" />
                </Section>
            </Layer>
        </StyleLayout>
